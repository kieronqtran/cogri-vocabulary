      "bull": "^3.5.2",
      "cache-manager": "^2.9.0",
      "cache-manager-redis-store": "^1.5.0",
      "class-transformer": "^0.2.0",
      "class-validator": "^0.9.1",
      "cookie-parser": "^1.4.3",
      "dotenv": "^6.1.0",
      "handlebars": "^4.0.12",
      "joi": "^14.3.0",
