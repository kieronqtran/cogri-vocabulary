      "prettier": "^1.14.2",
      "sql.js": "^0.5.0",
      "supertest": "^3.1.0",
      "ts-jest": "^23.1.3",
      "ts-loader": "^4.4.2",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.5.0",
      "tslint": "5.11.0",
      "typescript": "^3.0.1"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
