      "@types/cookie-parser": "^1.4.1",
      "@types/express": "^4.16.0",
      "@types/handlebars": "^4.0.40",
      "@types/jest": "^23.3.10",
      "@types/joi": "^14.0.0",
      "@types/node": "^8.0.29",
      "@types/nodemailer": "^4.6.5",
